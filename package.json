{
  "name": "frontend-config-scope",
  "version": "1.0.5",
  "homepage": "https://github.com/kibertoad/frontend-config-scope",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/frontend-config-scope.git"
  },
  "license": "Apache-2.0",
  "description": "Config management for the frontend",
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "config",
    "frontend",
    "validation"
  ],
  "scripts": {
    "build": "tsc",
    "build:release": "del-cli dist && del-cli coverage && tsc --project tsconfig.release.json",
    "test": "vitest",
    "test:karma": "",
    "test:coverage": "vitest --coverage",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:update-snapshots": "vitest -u",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build:release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.2.6",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.4",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3",
    "zod": "^3.21.4"
  }
}
